#include <stdio.h>
#include <stdlib.h>
#include <commons/log.h>
#include <commons/collections/list.h>
#include <commons/collections/dictionary.h>
#include <commons/config.h>
#include <string.h>

typedef struct
{
	char *numero;
}ip;

typedef struct
{
	ip ip_Kernel;
	int puerto_kernel;
}consola;


consola consola_crear(t_config* configuracion)
{
	consola consola_auxiliar;
	consola_auxiliar.ip_Kernel->numero=string_new();
	char* aux=strcpy(aux,"IP_KERNEL");
	char* puerto = strcpy(puerto, "PUERTO_KERNEL");
	consola_auxiliar.ip_Kernel.numero=config_get_string_value(configuracion,aux);
	consola_auxiliar.puerto_kernel=config_get_int_value(configuracion,puerto);
	return consola_auxiliar;

}



consola iniciarConsola(char *path)
{
	t_config *configuracion = malloc(sizeof(t_config));
	*configuracion = *config_create(path);
	free(configuracion);


}
int main(void) {

}
