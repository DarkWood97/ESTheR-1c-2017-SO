typedef struct {
	char *path;
	t_dictionary *properties;
}t_config;

typedef struct {
	int puerto;
	int marcos;
	int marco_size;
	int entradas_cache;
	int cache_x_proc;
	int retardo_memoria;
}memoria;

memoria memoriaCrear(t_config *configuracion){
	memoria memoriaAuxiliar;
	memoriaAuxiliar.puerto = config_get_int_value(*configuracion, )
	return memoriaAuxiliar;
}

void inicializarMemoria(char *path){
	t_config *configuracionMemoria;
	configuracionMemoria = config_create(path);
	memoria memoriaSistema = memoriaCrear(configuracionMemoria);

	
	



}

int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
